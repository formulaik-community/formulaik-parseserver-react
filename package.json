    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "react-dropzone-uploader": "^2.11.0",
    "react-flags-select": "^2.1.2"
  },
  "files": [
    "dist"
  ]
